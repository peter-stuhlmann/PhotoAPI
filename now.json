{
  "version": 2,
  "name": "photo-api",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "cache-control": "s-maxage=31536000, immutable"
      }
    }
  ],
  "env": {
    "DB_USERNAME": "***",
    "DB_PASSWORD": "***"
  }
}
